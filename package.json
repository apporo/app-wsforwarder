  "keywords": [
    "devebot",
    "devebot-plugin"
  ],
  "peerDependencies": {
    "app-websocket": "^0.2.0",
    "devebot": "^0.2.6"
  },
  "dependencies": {
    "app-websocket": "^0.2.0",
    "devebot": "^0.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-minify-mangle-names": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "mocha": "^5.1.1"
  }
}

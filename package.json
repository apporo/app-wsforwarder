{
  "name": "app-wsforwarder",
  "version": "0.2.0",
  "description": "Devebot mapping opflow-worker plugin",
  "homepage": "https://github.com/apporo/app-wsforwarder.git",
  "author": {
    "name": "apporo",
    "email": "apporo@devebot.com",
    "url": "https://github.com/apporo"
  },
  "main": "index.js",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "bin/",
    "config/",
    "data/",
    "lib/"
  ],
  "scripts": {
    "build": "babel ./src -d ./lib",
